# Pritunl Web UI Configuration
# These are normally set by pritunl-server when spawning the web process

# Server binding
BIND_HOST=0.0.0.0
BIND_PORT=443

# TLS (base64-encoded)
SSL_CERT=
SSL_KEY=

# Reverse proxy
REVERSE_PROXY_HEADER=X-Forwarded-For
REVERSE_PROXY_PROTO_HEADER=X-Forwarded-Proto
REDIRECT_SERVER=false

# Internal
INTERNAL_ADDRESS=

# Security
WEB_STRICT=true
WEB_SECRET=
